# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.TmpController.index

# biotop
GET     /store/users                controllers.BUsersController.index
POST    /store/users                controllers.BUsersController.create
GET     /store/users/:id            controllers.BUsersController.get(id :Long)
PATCH   /store/users/:id            controllers.BUsersController.patch(id :Long)
DELETE  /store/users/:id            controllers.BUsersController.delete(id :Long)
